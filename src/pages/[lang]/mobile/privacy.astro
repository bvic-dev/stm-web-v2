---
import Legal from '../../../components/mobile/legal.astro';
import { generateLangStaticPaths } from '../../../utils/locale';

const { lang } = Astro.params;

export function getStaticPaths() {
  return generateLangStaticPaths();
}

const privacy = await import(`../../../content/${lang}/mobile/privacy.md`);
---

<Legal legalMd={privacy} currentLocale={lang}/>